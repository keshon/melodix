# Discord Bot Token (Required)
DISCORD_TOKEN=INSERT_TOKEN_HERE

# Encoding Options for DCA

# Volume level for the audio encoding (default: 128)
ENCODE_VOLUME=128

# Number of audio channels (default: 2)
ENCODE_CHANNELS=2

# Frame rate for audio encoding (default: 48000)
ENCODE_FRAME_RATE=48000

# Frame duration in milliseconds (default: 20)
ENCODE_FRAME_DURATION=20

# Bitrate for encoding in kbps (default: 96)
ENCODE_BITRATE=96

# Compression level (default: 10, higher means more compression)
ENCODE_COMPRESSION_LEVEL=10

# Packet loss tolerance (default: 1)
ENCODE_PACKET_LOSS=1

# Number of buffered frames (default: 400)
ENCODE_BUFFERED_FRAMES=400

# Variable Bitrate (VBR) mode (default: true)
ENCODE_VBR=true

# Volume float value, where 1.0 is normal (default: 1.0)
ENCODE_VOLUME_FLOAT=1.0

# Whether to reconnect at end of file (default: 1)
ENCODE_RECONNECT_AT_EOF=1

# Whether to reconnect streamed data (default: 1)
ENCODE_RECONNECT_STREAMED=1

# Whether to reconnect on network errors (default: 1)
ENCODE_RECONNECT_ON_NETWORK_ERROR=1

# HTTP error codes to trigger reconnection (default: "4xx,5xx")
ENCODE_RECONNECT_ON_HTTP_ERROR="4xx,5xx"

# Max reconnect delay in seconds (default: 5)
ENCODE_RECONNECT_DELAY_MAX=5

# Path to the ffmpeg binary (default: empty string)
ENCODE_FFMPEG_BINARY_PATH=""

# Enable encoding line logging (default: true)
ENCODE_ENCODING_LINE_LOG=true

# User-Agent string for HTTP requests (default: Mozilla/4.0)
ENCODE_USER_AGENT="Mozilla/4.0 (compatible; MSIE 6.0; Windows NT 5.1; SV1)"

# Enable raw output mode (default: true)
ENCODE_RAW_OUTPUT=true